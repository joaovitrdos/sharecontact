{
  "expo": {
    "name": "sharecontact",
    "slug": "sharecontact",
    "scheme": "sharecontact",
    "version": "1.0.0",
    "plugins": [
      "expo-barcode-scanner",
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-barcode-scanner"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-transparent.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/logo-transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#232323"
    },
    "ios": {
      "newArchEnabled": true,
      "supportsTablet": true,
      "backgroundColor": "#232323"
    },
    "android": {
      "newArchEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo-transparent.png",
        "backgroundColor": "#232323"
      },
      "package": "com.anonymous.sharecontact"
    },
    "reactNativeNewArchitecture": true
  }
}
